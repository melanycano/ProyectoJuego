package com.mygdx.gorobots;

import com.badlogic.gdx.Game;
import com.badlogic.gdx.graphics.g2d.SpriteBatch;
import com.mygdx.gorobots.pantallas.PantallaCarga;
import com.mygdx.gorobots.utiles.Render;

public class GoRobots extends Game {
	
	public static final int vWidth = 400;
	public static final int vHeight = 208;
	
	public void create () {
		Render.app = this;
		Render.sb = new SpriteBatch();
		this.setScreen(new PantallaCarga());
	}

	
	@Override
	public void render () {
		super.render();
	}
	

	public void dispose () {
		Render.sb.dispose();
		this.getScreen();
	}
}
